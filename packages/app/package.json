{
  "name": "@seldon/app",
  "private": true,
  "description": "Seldon App",
  "version": "0.0.1",
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "scripts": {
    "check:types": "tsc --build tsconfig.json --noEmit",
    "check:types:trace": "tsc --noEmit --generateTrace ts-trace --incremental false",
    "env": "dotenvx run -f ../../.env.local",
    "dev": "npm run env -- -- npm run dev",
    "dev:local": "tsx --watch ./src/index.ts",
    "dev:format": "prettier --write ./src",
    "build": "tsc --build tsconfig.json",
    "start": "node dist/index.js",
    "start:migrate": "prisma migrate deploy && node dist/index.js"
  },
  "imports": {
    "#db": "./client/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.741.0",
    "@hono/node-server": "^1.13.8",
    "@hono/zod-validator": "0.7.0",
    "@prisma/adapter-pg": "^6.13.0",
    "@prisma/client": "^6.13.0",
    "consola": "^3.4.0",
    "cryptr": "^6.3.0",
    "file-type": "^20.1.0",
    "hono": "^4.6.20",
    "mime-types": "^2.1.35",
    "nanoid": "3.3.8",
    "uuid": "^11.1.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.4.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.19.0",
    "eslint-plugin-bedrock": "^1.0.26",
    "prisma": "^6.13.0",
    "prisma-json-types-generator": "^3.5.2",
    "tsx": "4.19.2",
    "typescript": "^5.5.4"
  },
  "volta": {
    "node": "22.18.0"
  }
}
