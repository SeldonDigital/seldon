{
  "name": "@seldon/base",
  "version": "0.0.0",
  "description": "Open-source design system and component framework designed for building component-driven UIs with type-safe design tokens",
  "type": "module",
  "scripts": {
    "check:types": "tsc --build ./tsconfig.json",
    "env": "dotenvx run -f .env.local",
    "dev": "npm run env -- -- node ./scripts/dev.ts",
    "dev:api": "tsx --watch ./packages/api/index.ts",
    "dev:ui": "vite",
    "build:editor": "vite build",
    "build:api": "tsc --build ./packages/api/tsconfig.build.json",
    "build": "npm run build:editor && npm run build:api",
    "format": "prettier --write \"**/*.{ts,tsx,json,css}\"",
    "start": "node dist/index.js",
    "start:migrate": "prisma migrate deploy && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeldonDigital/seldon.git"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN ./docs/licenses/noncommercial/LICENSE",
  "bugs": {
    "url": "https://github.com/SeldonDigital/seldon/issues"
  },
  "homepage": "https://github.com/SeldonDigital/seldon#readme",
  "workspaces": [
    "packages/core",
    "packages/factory"
  ],
  "imports": {
    "#db": "./client/index.js"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
